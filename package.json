{
  "name": "jays-mobile-wash",
  "version": "2.0.0",
  "private": true,
  "description": "Premium mobile car detailing service with 3D McLaren visualization and interactive effects",
  "keywords": [
    "mobile car wash",
    "car detailing",
    "ceramic coating",
    "McLaren",
    "3D visualization",
    "Los Angeles",
    "Orange County"
  ],
  "homepage": "https://jaysmobilewash.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/jjmandog/jaysmobilewash.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,scss,json}",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && gh-pages -d dist",
    "analyze": "npx vite-bundle-analyzer",
    "serve": "npx serve dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-helmet-async": "^2.0.4",
    "framer-motion": "^10.18.0",
    "three": "^0.160.0",
    "@react-three/fiber": "^8.15.0",
    "@react-three/drei": "^9.92.0",
    "react-spring": "^9.7.0",
    "howler": "^2.2.4",
    "lottie-react": "^2.4.0",
    "react-intersection-observer": "^9.5.3",
    "react-use": "^17.4.0",
    "date-fns": "^2.30.0",
    "react-hot-toast": "^2.4.1",
    "react-confetti": "^6.1.0",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/three": "^0.160.0",
    "@types/howler": "^2.2.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.17.5",
    "vitest": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "@vitest/coverage-v8": "^1.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}